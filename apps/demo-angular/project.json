{
  "name": "demo-angular",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/demo-angular/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "dist/demo-angular",
        "index": "apps/demo-angular/src/index.html",
        "main": "apps/demo-angular/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/demo-angular/tsconfig.app.json",
        "inlineStyleLanguage": "sass",
        "assets": [],
        "styles": [
          "apps/demo-angular/src/styles.sass",
          "packages/angular-cli/src/assets/styles.sass"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "1MB",
              "maximumError": "2MB"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production",
      "outputs": ["{workspaceRoot}/dist/demo-angular"]
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "proxyConfig": "proxy.conf.json"
      },
      "configurations": {
        "production": {
          "buildTarget": "demo-angular:build:production"
        },
        "development": {
          "buildTarget": "demo-angular:build:development"
        }
      },
      "defaultConfiguration": "development"
    }
  }
}
