{
  "name": "regex",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/plugins/regex/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "tsup",
          "cp package.json ../../../dist/regex/package.json"
        ],
        "cwd": "packages/plugins/regex",
        "parallel": false
      },
      "inputs": ["production", "sharedGlobals"],
      "outputs": ["{workspaceRoot}/dist/regex"]
    }
  }
}
