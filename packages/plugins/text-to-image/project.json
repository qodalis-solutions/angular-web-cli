{
  "name": "text-to-image",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/plugins/text-to-image/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "tsup",
          "cp package.json ../../../dist/text-to-image/package.json"
        ],
        "cwd": "packages/plugins/text-to-image",
        "parallel": false
      },
      "inputs": ["production", "sharedGlobals"],
      "outputs": ["{workspaceRoot}/dist/text-to-image"]
    }
  }
}
