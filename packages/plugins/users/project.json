{
  "name": "users",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/plugins/users/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "tsup",
          "cp package.json ../../../dist/users/package.json"
        ],
        "cwd": "packages/plugins/users",
        "parallel": false
      },
      "inputs": ["production", "sharedGlobals"],
      "outputs": ["{workspaceRoot}/dist/users"]
    }
  }
}
