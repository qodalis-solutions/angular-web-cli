{
  "name": "@qodalis/react-cli",
  "version": "2.0.1",
  "description": "React wrapper for the @qodalis CLI terminal engine.",
  "author": "Nicolae Lupei, Qodalis Solutions",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/qodalis-solutions/web-cli"
  },
  "homepage": "https://qodalis.com",
  "keywords": [
    "react",
    "cli",
    "qodalis",
    "terminal",
    "web-terminal",
    "xterm"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@qodalis/cli": "^2.0.1",
    "@qodalis/cli-core": "^2.0.1"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "@types/react": "^18.0.0 || ^19.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "sideEffects": false
}
